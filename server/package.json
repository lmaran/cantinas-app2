    "scripts": {
        "start": "npm run clean && npm run start-server",
        "start-server": "npm run build-server && npm run watch-server",
        "watch-server": "run-p \"watch-server-src\" \"watch-server-dist\"",
        "watch-server-src": "tsc -p ./tsconfig.dev.json --watch",
        "watch-server-dist": "nodemon --config nodemon.json",
        "build": "npm run clean && npm run build-server",
        "build-server": "tsc -p ./tsconfig.dev.json",
        "build-prod": "npm run clean && npm run build-prod-server",
        "build-prod-server": "tsc -p ./tsconfig.prod.json",
        "lint": "tslint './src/**/*.ts' -c ./tslint.json  --format stylish --force",
        "clean": "rimraf dist",
        "test": "echo \"Error: no test specified\" && exit 1"
